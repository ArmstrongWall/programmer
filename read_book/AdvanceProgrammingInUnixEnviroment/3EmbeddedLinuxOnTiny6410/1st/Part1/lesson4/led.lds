SECTION
{
	. = 0x30008000;//"."表示程序中当前的地址设置程序的起始链接地址
	. =ALIGN(4);//设置代码段4字节对齐，如果以上的地址没有4字节对齐
                    //就会加上几个字节使之满足条件
	
	.text ://代码段
	{
	start.o(.text)//设置start.o为第一个执行的文件
	*(.text)
	}

	. =ALIGN(4);//都要设置四字节对齐
	
	.data :     //数据段
	{
	*(.data)
	}
	
	bss_start = . ;//当前代码段的地址赋值给bss_start
	. =ALIGN(4);   //都要设置四字节对齐
	.bss :         //bss段
	{
	*(.bss)
	}
	bss_end = . ;

}
